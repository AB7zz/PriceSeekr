{
  "name": "PriceSeekr",
  "displayName": "PriceSeekr",
  "version": "0.0.1",
  "description": "Look up similiar products from google",
  "author": "Abhinav & Abbas",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "verbose": "plasmo dev --verbose"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.6",
    "@plasmohq/storage": "^1.6.1",
    "@propelauth/react": "^2.0.10",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "firebase": "^10.3.0",
    "google-search-results-nodejs": "^2.1.0",
    "plasmo": "0.76.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.14.1",
    "serp": "^2.2.2",
    "tailwindcss": "3.3.2",
    "wikipedia-tldr": "^2.0.1"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.237",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "typescript": "5.1.3"
  },
  "manifest": {
    "key": "$CRX_PUBLIC_KEY",
    "oauth2": {
      "client_id": "$PLASMO_PUBLIC_FIREBASE_CLIENT_ID",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    },
    "host_permissions": [
      "https://*/*",
      "apis.google.com/*"
    ],
    "permissions": [
      "browsingData",
      "activeTab",
      "contextMenus",
      "tabs",
      "declarativeContent",
      "scripting",
      "storage",
      "identity"
    ]
  }
}
